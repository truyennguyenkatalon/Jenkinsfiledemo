pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello Build from GitHub'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello Deploy'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello Test'
            }
        }
        stage('Release') {
            steps {
                echo 'Release World'
            }
        }
    }
}
